public class Customer {
    // Attributes
    String name; // Name, displayed on receipt
    String num; // Phone number, displayed on receipt
    String mail; // Email, displayed on receipt
    String address; // Home address, displayed on receipt
    String location; // City, displayed on receipt

    // Methods
    // Constructor
    Customer(String name, String phone, String email, String home, String city) {
        this.name = name;
        this.num = phone;
        this.mail = email;
        this.address = home;
        this.location = city;
    }

    // Return location, used to find driver
    String getLocation() {
        return location;
    }
}